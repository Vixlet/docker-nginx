server {
    listen       ${NGINX_PORT};
    server_name  ${NGINX_HOST};
    autoindex    ${DEVELOPMENT_MODE};
    location / {
        root   /usr/share/nginx/html;
        index  index.html index.htm;
        include  /etc/nginx/includes.d/cors.conf;
    }
    error_page   500 502 503 504  /50x.html;
    location = /50x.html {
        root   /usr/share/nginx/html;
    }
}